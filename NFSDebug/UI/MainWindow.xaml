<Window x:Class="NFSDebug.UI.MainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NFSDebug.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="Window_Loaded">
    <Grid>
        <WrapPanel Orientation="Vertical" Margin="5">
            <TextBlock HorizontalAlignment="Left" VerticalAlignment="Top" Margin="2" FontSize="14">NFS Debug by Dz3n</TextBlock>
            <TextBlock Margin="2">Choose NFS</TextBlock>
            <ListBox x:Name="lbProcesses" HorizontalAlignment="Stretch" Margin="2"></ListBox>
        </WrapPanel>
    </Grid>
</Window>
